
 <a href="./message.handlebars" class="btn btn-primary"> Chat</a>

<a href="./form.handlebars" class="btn btn-primary"> + New Post</a>

 
 <div class="card text-center">
    
                {{#each content}}
        <div class="card-body">
            <p class="card-text">
            <h5>{{user_name}}</h5>
                {{#if resident}}
                <li>Resident: {{ resident }}</li>
                {{else}}
                <li>Retailer: {{ retailer }}</li>
                {{/if}}
                <li>Zipcode: {{{ zip_code }}}</li>
                {{#if test_seeking}}
                <li>Looking for Tests: {{{ tests_seeking }}}</li>
                {{else}}
                <li>Tests Available: {{{ tests_available }}}</li>
                {{/if}}
                <li>Comments: {{ post_comment }}</li>
                <li>Contact me at: {{ contact_me }}</li>
                <li>{{ date_created }}</li>
            </p>
        </div>
                {{/each}}
    </div>