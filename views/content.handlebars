
    <div class="card text-center" style="background-color: #e3f2fd;;border:solid">
        {{#each content}}
        <div class="card-body">
            <p class="card-text">
            <h5>{{user_name}}</h5>
            {{#if resident}}
            <li>Resident:{{resident}}</li>
            {{else}}
            <li>Retailer:{{retailer}}</li>
            {{/if}}
            <li>Zipcode:{{{zip_code}}}</li>
            {{#if test_seeking}}
            <li>Looking for Tests:{{{tests_seeking}}}</li>
            {{else}}
            <li>Tests Available:{{{tests_available}}}</li>
            {{/if}}
            <li>Comments:{{post_comment}}</li>
            <li>Contact me at:{{contact_me}}</li>
            </p>
        </div>
        {{/each}}
        <div class="card-footer text-muted">
            <p>Created by {{user.name}} on {{date}}</p>
        </div>
    </div>
